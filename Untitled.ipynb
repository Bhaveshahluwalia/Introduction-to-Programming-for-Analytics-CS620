    "def prime1(n):\n",
    "    pri=list()\n",
    "    pri.append([2])\n",
    "    for num in range(3,n+1,2):\n",
    "        if all(num% i!=0 for i in range(2,int (num**.5) +1)):\n",
    "            pri.append([num])\n",
    "    return pri\n",
    "prime1(13)"
